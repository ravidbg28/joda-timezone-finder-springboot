<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tact.base.mybatis.mapper.TimezoneMapper">
    
    <select id="findByNameLike" resultType="org.tact.base.mybatis.domain.Timezone" parameterType="string">
    	SELECT * FROM TIMEZONE WHERE NAME LIKE #{NAME}        
    </select>
    
</mapper>
